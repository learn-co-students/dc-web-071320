<h1>Missions#new</h1>
<p>Find me in app/views/missions/new.html.erb</p>

<%= form_for @mission do |f| %>
    <%= f.label :name %>    
    <%= f.text_field :name %>
    <%= f.label :planet %>    
    <%= f.collection_select :planet_id, Planet.all, :id, :name %>
    <%= f.label :scientist %>   
    <%= f.collection_select :scientist_id, Scientist.all, :id, :name  %>
    <%= f.submit %>
<% end %>