<div class="header">
    <% unless params[:controller] == "sessions" &&  params[:action] == "login" %>

        <% if logged_in? %>
            Welcome to our Taco Tracker App, <%= current_user.username %>
            <%= link_to "Logout", logout_path, method: :delete %>
        <% else %>
            <%= link_to "Login", login_path %>
        <% end %>
    <% end %>
</div>